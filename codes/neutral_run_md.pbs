#!/bin/sh

#PBS -l select=1:ncpus=1:mem=16gb  
#PBS -j oe
#PBS -m e

bin=$bin
molname=$PBS_JOBNAME

export OMP_NUM_THREADS=1

if [ -f "$PBS_O_WORKDIR/qcxms.gs" ]; then
    echo "qcxms.gs already exists"
    exit
fi

cp -r $PBS_O_WORKDIR/$molname.xyz $SCRATCHDIR
cp -r $PBS_O_WORKDIR/qcxms.in $SCRATCHDIR


cd $SCRATCHDIR

$bin/qcxms_v5.2.1 -i $molname.xyz  >$molname'_neutral.out'

cp -r $SCRATCHDIR/* $PBS_O_WORKDIR
